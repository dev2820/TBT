{
  "presets": ["module:metro-react-native-babel-preset"],
  "plugins": [
    [
      "module-resolver",
      {
        "extensions": [".ios.js", ".android.js", ".js", ".ts", ".tsx", ".json"],
        "root": ["."],
        "alias": {
          "@": "./src",
          "@assets/*": ["./src/assets"],
          "@router/*": ["./src/router"],
          "@components/*": ["./src/components"],
          "@containers/*": ["./src/containers"],
          "@screens/*": ["./src/screens"],
          "@utils/*": ["./src/utils"]
        }
      }
    ]
  ]
}
